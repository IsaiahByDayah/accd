{
	"name": "cra-typescript",
	"version": "0.1.0",
	"scripts": {
		"start": "react-scripts start",
		"build": "react-scripts build",
		"test": "react-scripts test",
		"eject": "react-scripts eject",
		"lint": "eslint './src/**/*.{ts,tsx}'",
		"format": "prettier --config ./.prettierrc.json --ignore-path ./.prettierignore --write '**/*.tsx' '**/*.ts'",
		"storybook": "start-storybook -p 9009 -s public --ci",
		"build-storybook": "build-storybook -s public",
		"serve-storybook": "yarn build-storybook && npx serve storybook-static"
	},
	"eslintConfig": {
		"extends": "react-app"
	},
	"browserslist": {
		"production": [
			">0.2%",
			"not dead",
			"not op_mini all"
		],
		"development": [
			"last 1 chrome version",
			"last 1 firefox version",
			"last 1 safari version"
		]
	},
	"dependencies": {
		"@material-ui/core": "^4.5.0",
		"@material-ui/icons": "^4.4.3",
		"@material-ui/lab": "^4.0.0-alpha.28",
		"react": "^16.10.2",
		"react-dom": "^16.10.2",
		"react-router-dom": "^5.1.2",
		"react-scripts": "3.2.0",
		"react-test-renderer": "^16.10.2",
		"typescript": "3.6.4"
	},
	"devDependencies": {
		"@storybook/addon-a11y": "^5.2.3",
		"@storybook/addon-actions": "^5.2.3",
		"@storybook/addon-backgrounds": "^5.2.3",
		"@storybook/addon-console": "^1.2.1",
		"@storybook/addon-knobs": "^5.2.3",
		"@storybook/addon-links": "^5.2.3",
		"@storybook/addon-notes": "^5.2.3",
		"@storybook/addon-storyshots": "^5.2.3",
		"@storybook/addon-viewport": "^5.2.3",
		"@storybook/addons": "^5.2.3",
		"@storybook/react": "^5.2.3",
		"@types/jest": "24.0.18",
		"@types/node": "12.7.12",
		"@types/react": "16.9.5",
		"@types/react-dom": "16.9.1",
		"@types/react-router-dom": "^5.1.0",
		"@types/react-test-renderer": "^16.9.0",
		"@types/storybook__addon-storyshots": "^5.1.1",
		"@types/storybook__react": "^4.0.2",
		"@typescript-eslint/eslint-plugin": "^2.3.3",
		"@typescript-eslint/parser": "^2.3.3",
		"eslint-config-prettier": "^6.4.0",
		"eslint-config-react": "^1.1.7",
		"eslint-plugin-prettier": "^3.1.1",
		"eslint-plugin-react-hooks": "^2.1.2",
		"prettier": "^1.18.2",
		"require-context.macro": "^1.2.2"
	}
}
